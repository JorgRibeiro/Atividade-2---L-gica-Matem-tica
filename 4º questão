#include <stdio.h>

// autor: Jorge Alexandre Ribeiro Vilela Júnior

int main() {

  int n, primo = 0;

  scanf("%d",&n);

  for(int i = 2; i < n; i++){
    if(n % i == 0){
     primo = 1;
    }
  }

  if(primo == 1){
    printf("O número %d não é primo.",n);
  } else {printf("O número %d é primo.",n);}
  
  
  return 0;
}

